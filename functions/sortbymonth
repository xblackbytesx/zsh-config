function sortbymonth {
  currentYear=${PWD##*/}

  echo "Sorting photos by month for year $YEAR"

  for month in {01..12}
    do
      echo "Create dir and move photo's for month $month"
      mkdir ./$month
      mv *$currentYear$month**_* $month/
      mv *$currentYear$month*********** $month/
    done
}
